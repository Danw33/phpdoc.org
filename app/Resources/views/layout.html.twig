                        <ul class="dropdown-menu">
                            <li><a href="http://demo.phpdoc.org/Responsive/"><i class="icon-list-alt"></i> Responsive</a></li>
                        </ul>
                    </li>
                    <li><a href="{{ path('template_list') }}"><i class="icon-list-alt"></i> Templates</a></li>
                    <li><a href="{{ path('contact') }}"><i class="icon-list-alt"></i> Contact</a></li>
                    <li><a href="{{ path('about') }}"><i class="icon-list-alt"></i> About</a></li>
                </ul>
            </div>
        </div>
    </div>
    <div class="go_to_top"><a href="#___" style="color: inherit">Back to top <i class="icon-upload icon-white"></i></a></div>
</div>
<div id="___" class="container">
